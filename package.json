{
	"name": "n8n-nodes-starter",
	"version": "0.1.0",
	"description": "",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "",
		"email": ""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GNIVC/n8n-nodes-starter.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "cross-env N8N_DEV_RELOAD=true n8n-node dev",
		"dev:watch": "npm run build:watch",
		"dev:manual": "npm run build && npx --yes n8n start",
		"dev:debug": "cross-env N8N_DEV_RELOAD=true NODE_OPTIONS=\"--inspect=0.0.0.0:9229\" n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"postinstall": "npx patch-package || exit 0"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/GithubIssuesApi.credentials.js",
			"dist/credentials/GithubIssuesOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/GithubIssues/GithubIssues.node.js",
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/ArrayCondition/ArrayCondition.node.js",
			"dist/nodes/ArrayIfThen/ArrayIfThen.node.js",
			"dist/nodes/HeaderCondition/HeaderCondition.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"eslint": "9.32.0",
		"patch-package": "^8.0.1",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
